@using Falcon.Web.Mvc.HtmlExtensions
@model Vino.Server.Services.MainServices.CRM.AirImp.Models.AirImpModel

<head>
    <script src="/wwwroot/js/kendo/2014.1.528/kendo.all.min.js"></script>
    <script src="/wwwroot/js/kendo/2014.1.528/kendo.datepicker.min.js"></script>
    <link rel="stylesheet" href="/wwwroot/js/kendo/2014.1.318/kendo.common.min.css" />
    <link rel="stylesheet" href="/wwwroot/js/kendo/2014.1.318/kendo.default.min.css" />
    <link rel="stylesheet" href="/wwwroot/js/kendo/2014.1.318/kendo.default.mobile.min.css" />
</head>
@{
    Layout = null;
}
<body>

    <section class="wr-main">
        <div class="out-content-mid no-mb no-minheight">
            <div class="container-fluid">
                <div class="row">

                    @*Job Id*@
                    <div class="col-sm-12 col-md-6 col-lg-4">
                        <div class="out-input">
                            <label>Job ID</label>
                            @Html.EditorFor(model => model.JobId, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                            <div id="div-error-message-item" class="color-danger p-t-10" style="display: none;">Please input data</div>
                        </div>
                    </div>

                    @*OPIC*@
                    <table class="col-sm-12 col-md-6 col-lg-4">
                        <tr class="out-input">
                            <th>O.P.I.C</th>
                        <tr>
                            <td>
                                @Html.DropDownListFor(model => model.OpIcId, new SelectList(Model.ContactItems, "Value", "Text"),
                                         new { @Id = "OpicId", @class = "form-control select-style-1 DrpContact" })
                        </td>
                        <td><a data-request-url="@Url.Action("OpenModal", "Contact")" id="create-opic">Add</a></td>
                    </tr>
                    </tr>

                </table>

                @*AirLines*@

                <table class="col-sm-12 col-md-6 col-lg-4">
                    <tr class="out-input">
                        <th>AirLines</th>
                    <tr>
                        <td>
                            @Html.DropDownListFor(model => model.AirlineId, new SelectList(Model.CarrierItems, "Value", "Text"),
                                     new { @Id = "AirLineId", @class = "form-control select-style-1 DrpCarrier" })
                        </td>
                        <td><a data-request-url="@Url.Action("OpenModal", "Carrier")" id="create-airlines">Add</a></td>
                    </tr>
                    </tr>

                </table>
                @*created*@
                <div class="col-sm-12 col-md-6 col-lg-2">
                    <div class="out-input">
                        <label>Create</label>
                        <div class="out-datapick">
                            <input type="text" name="@Html.NameFor(model => model.Created)" value="@Model.Created" class="form-control mydatepicker">
                            <span class="input-group-addon"><i class="mdi mdi-calendar-clock"></i></span>
                            @*<input id="datetimepickerr" title="datetimepicker" style="width: 100%;"/>*@
                            @*<script>
                                    $(document).ready(function () {
                                        $("#datetimepickerr").kendoDateTimePicker({
                                            value: @Model.Created.Date,
                                            format: "dd/MM/yyyy",
                                            dateInput: false,

                                        }).addClass("form-control");
                                    });
                                </script>*@
                        </div>

                    </div>

                </div>
                @*eta*@
                <div class="col-sm-12 col-md-6 col-lg-2">
                    <div class="out-input">
                        <label>Eta</label>
                        <div class="out-datapick">
                            <input type="text" name="@Html.NameFor(model => model.Eta)" value="@Model.Eta" class="form-control mydatepicker">
                            <span class="input-group-addon"><i class="mdi mdi-calendar-clock"></i></span>
                        </div>

                        @*<input id="datetimepicker" title="datetimepicker" />
                            <script>
                                $(document).ready(function () {
                                    $("#datetimepicker").kendoDateTimePicker({
                                        value: @Model.Eta,
                                        format: "dd/MM/yyyy HH:mm:ss",
                                        dateInput: false
                                    });
                                });
                            </script>*@

                    </div>
                </div>

                @*Service*@
                <div class="col-sm-12 col-md-6 col-lg-4">
                    <div class="out-input">
                        <label>Service</label>
                        @Html.EditorFor(model => model.Service, new { htmlAttributes = new { @class = "form-control" } })
                        <div id="div-error-message-item" class="color-danger p-t-10" style="display: none;">Please input data</div>
                    </div>
                </div>

                @*agent*@
                <table class="col-sm-12 col-md-6 col-lg-4">
                    <tr class="out-input">
                        <th>Agent</th>
                    <tr>
                        <td>
                            @Html.DropDownListFor(model => model.AgentId, new SelectList(Model.CarrierItems, "Value", "Text"),
                                     new { @Id = "AgentId", @class = "form-control select-style-1 DrpCarrier" })
                        </td>
                        <td><a data-request-url="@Url.Action("OpenModal", "Carrier")" id="create-agent">Add</a></td>
                    </tr>
                    </tr>
                  
                </table>
                @*Mawb No*@
                <div class="col-sm-12 col-md-6 col-lg-2">
                    <div class="out-input">
                        <label>Mawb No</label>
                        @Html.EditorFor(model => model.MawbNumber, new { htmlAttributes = new { @class = "form-control" } })
                        <div id="div-error-message-item" class="color-danger p-t-10" style="display: none;">Please input data</div>
                    </div>
                </div>

                @*Bill Type*@
                <div class="col-sm-12 col-md-6 col-lg-2">
                    <div class="out-input">
                        <label>Bill Type</label>
                        @Html.DropDownListFor(model => model.TypeOfBillId, new SelectList(Model.TypeOfBillItems, "Value", "Text"), new { @class = "form-control select-style-1" })

                    </div>
                </div>

                @*AOL*@
                <table class="col-sm-12 col-md-6 col-lg-4">
                    <tr class="out-input">
                        <th>A.O.L</th>
                        <tr>
                            <td>@Html.DropDownListFor(model => model.AolId, new SelectList(Model.PortItems, "Value", "Text"),
                                    new { @Id = "AolId", @class = "form-control select-style-1 DrpPort" })</td>
                            <td><a data-request-url="@Url.Action("OpenModal", "Port")" id="create-aol">Add</a></td>
                        </tr>
                    </tr>
                </table>
                @*AOD*@
                <table class="col-sm-12 col-md-6 col-lg-4">
                    <tr class="out-input">
                        <th>A.O.D</th>
                        <tr>
                            <td>@Html.DropDownListFor(model => model.AolId, new SelectList(Model.PortItems, "Value", "Text"),
                                    new { @Id = "AodId", @class = "form-control select-style-1 DrpPort" })</td>
                            <td><a data-request-url="@Url.Action("OpenModal", "Port")" id="create-aod">Add</a></td>
                        </tr>
                    </tr>
                </table>
                @*Flight No*@
                <div class="col-sm-12 col-md-6 col-lg-2">
                    <div class="out-input">
                        <label>Flight Noo</label>
                        @Html.EditorFor(model => model.FlightNumber, new { htmlAttributes = new { @class = "form-control" } })
                        <div id="div-error-message-item" class="color-danger p-t-10" style="display: none;">Please input data</div>
                    </div>
                </div>

                @*Fly Date*@
                <div class="col-sm-12 col-md-6 col-lg-2">
                    <div class="out-input">
                        <label>Fly Date</label>
                        <div class="out-datapick">
                            <input type="text" name="@Html.NameFor(model => model.FLyDate)" value="@Model.FLyDate" class="form-control mydatepicker">
                            <span class="input-group-addon"><i class="mdi mdi-calendar-clock"></i></span>
                        </div>
                    </div>
                </div>

                @*Delivery*@
                <table class="col-sm-12 col-md-6 col-lg-4">
                    <tr class="out-input">
                        <th>Delivery</th>
                        <tr>
                            <td>@Html.DropDownListFor(model => model.AolId, new SelectList(Model.PortItems, "Value", "Text"),
                                    new { @Id = "DeliveryId", @class = "form-control select-style-1 DrpPort" })</td>
                            <td><a data-request-url="@Url.Action("OpenModal", "Port")" id="create-delivery">Add</a></td>
                        </tr>
                    </tr>
                </table>
                @*Routing*@
                <div class="col-sm-12 col-md-6 col-lg-4">
                    <div class="out-input">
                        <label>Routing</label>
                        @Html.EditorFor(model => model.Routing, new { htmlAttributes = new { @class = "form-control" } })
                        <div id="div-error-message-item" class="color-danger p-t-10" style="display: none;">Please input data</div>
                    </div>
                </div>

                @*commodity*@
                <div class="col-sm-12 col-md-6 col-lg-4">
                    <div class="out-input">
                        <label>Commodity</label>
                        @Html.DropDownListFor(model => model.CommodityId, new SelectList(Model.CommodityItems, "Value", "Text"), new { @class = "form-control select-style-1" })

                        @*<select id="Commodity" name="@Html.NameFor(model => model.CommodityId)" class="form-control select-style-1"></select>*@
                    </div>
                </div>

                @*Quantity*@
                <div class="col-sm-12 col-md-6 col-lg-2">
                    <div class="out-input">
                        <label>Quantity</label>
                        @Html.EditorFor(model => model.Quantity, new { htmlAttributes = new { @class = "form-control" } })
                        <div id="div-error-message-item" class="color-danger p-t-10" style="display: none;">Please input data</div>
                    </div>
                </div>

                @*Unit*@
                <div class="col-sm-12 col-md-6 col-lg-2">
                    <div class="out-input">
                        <label>Unit</label>
                        @Html.DropDownListFor(model => model.UnitId, new SelectList(Model.UnitItems, "Value", "Text"), new { @class = "form-control select-style-1" })
                    </div>
                </div>

                @*SCN*@
                <div class="col-sm-12 col-md-6 col-lg-4">
                    <div class="out-input">
                        <label>SCN</label>
                        @Html.EditorFor(model => model.Scn, new { htmlAttributes = new { @class = "form-control" } })
                        <div id="div-error-message-item" class="color-danger p-t-10" style="display: none;">Please input data</div>
                    </div>
                </div>
                @*Shipments*@
                <div class="col-sm-12 col-md-6 col-lg-3">
                    <div class="out-input">
                        <label>Shipments</label>
                        @Html.DropDownListFor(model => model.ShipmentId, new SelectList(Model.ShipmentItems, "Value", "Text"), new { @class = "form-control select-style-1" })
                    </div>
                </div>

                @*isFinish*@
                <div class="col-sm-12 col-md-3 col-lg-1">
                    <div class="out-input">
                        <div>
                            @Html.CheckBoxFor(m => m.IsFinish, new { @class = "checkbox checkbox-primary" })
                            @*<input id="checkbox1" value="@Html.ValueFor(m => m.IsFinish)" type="checkbox" name="@Html.NameFor(model => model.IsFinish)">*@

                            <label for="@Model.IsFinish">Iss Finished</label>
                        </div>
                    </div>
                </div>

                @*GW*@
                <div class="col-sm-12 col-md-6 col-lg-2">
                    <div class="out-input">
                        <label>G.W</label>
                        @Html.EditorFor(model => model.Gw, new { htmlAttributes = new { @class = "form-control" } })
                        <div id="div-error-message-item" class="color-danger p-t-10" style="display: none;">Please input data</div>
                    </div>
                </div>
                @*CW*@
                <div class="col-sm-12 col-md-6 col-lg-2">
                    <div class="out-input">
                        <label>CW</label>
                        @Html.EditorFor(model => model.Cw, new { htmlAttributes = new { @class = "form-control" } })
                        <div id="div-error-message-item" class="color-danger p-t-10" style="display: none;">Please input data</div>
                    </div>
                </div>

                @*Notes*@
                <div class="col-sm-12 col-md-6 col-lg-4">
                    <div class="out-input">
                        <label>Notes</label>
                        @Html.EditorFor(model => model.Notes, new { htmlAttributes = new { @class = "form-control" } })
                        @*<input type="text" class="form-control" placeholder="">*@
                        <div id="div-error-message-item" class="color-danger p-t-10" style="display: none;">Please input data</div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @*modal*@
    <div id="myModal" class="modal  fade " data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div id="myModalContent"></div>
            </div>
        </div>
    </div>

</section>
<script src="/Areas/Admin/Content/js/airimps_addedit.js"></script>
</body>



