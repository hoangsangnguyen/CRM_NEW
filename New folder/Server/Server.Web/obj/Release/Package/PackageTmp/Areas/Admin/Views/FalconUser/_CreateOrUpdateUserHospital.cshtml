@using Falcon.Web.Mvc.HtmlExtensions
@model Vino.Server.Services.MainServices.Users.Models.UserHospitalModel

@Html.ValidationSummary(true)
@Html.HiddenFor(model => model.Id)

<div class="content">
    <div class="form-horizontal">
        <div id="Employee-edit" class="nav-tabs-custom">
            <ul class="nav nav-tabs">
                @Html.RenderBootstrapTabHeader("tab-info", new LocalizedString("Thông tin"), true)
            </ul>
            <div class="tab-content">
                <div class="panel-group">
                    <div class="panel panel-default">
                        <div class="panel-body">
                          
                            <div class="form-group">
                                <div class="col-md-3">
                                    <label class="control-label">User</label>
                                </div>
                                <div class="col-md-9">
                                    @Html.FalconEditorFor(model => model.UserId)
                                    @Html.ValidationMessageFor(model => model.UserId)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-3">
                                    <label class="control-label">Bệnh viện</label>
                                </div>
                                <div class="col-md-9">
                                    @Html.FalconEditorFor(model => model.HospitalId)
                                    @Html.ValidationMessageFor(model => model.HospitalId)
                                </div>
                            </div>
                           
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

