                            <i class="fa fa-industry"></i>
                            <span style="color: #0173b7">HBL</span>
                            <span class="pull-right-container" style="color: #0173b7">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">

                            <li class="@Html.IsActive("HblLclExp","Index")">
                                <a href="@Url.Action("Index", "HblLclExp")">
                                    <i class="fa fa-arrow-circle-right" style="margin-left: 5px"></i>
                                    <span style="color: #0173b7">Lcl Hbl</span>
                                </a>
                            </li>
                        </ul>
                    </li>

                    <li class="@Html.IsActive("Lookup","LookupList")">
                        <a href="@Url.Action("LookupList", "Lookup")">
                            <i class="fa fa-industry" style="margin-left: 5px"></i>
                            <span style="color: #0173b7">Lookup</span>
                        </a>
                    </li>

                </ul>
