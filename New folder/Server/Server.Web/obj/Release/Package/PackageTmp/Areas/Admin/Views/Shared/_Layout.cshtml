                            <span style="color: #0173b7">Port</span>
                        </a>
                    </li>

                    <li class="@Html.IsActive("Carrier","Index")">
                        <a href="@Url.Action("Index", "Carrier")">
                            <i class="fa fa-industry" style="margin-left: 5px"></i>
                            <span style="color: #0173b7">Carrier</span>
                        </a>
                    </li>

                    <li class="treeview
                        @Html.IsActive("Producing", "InternalProductionManage,ExternalProductionManage")
                        @Html.IsHidden("ManageEmployee,ManageSupplier,ManageCustomer,ManageProduct")">
                        <a href="#">
                            <i class="fa fa-industry"></i>
                            <span style="color: #0173b7">Express</span>
                            <span class="pull-right-container" style="color: #0173b7">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">

                            <li class="@Html.IsActive("AirExps","Index")">
                                <a href="@Url.Action("Index", "AirExps")">
                                    <i class="fa fa-arrow-circle-right" style="margin-left: 5px"></i>
                                    <span style="color: #0173b7">AirExps</span>
                                </a>
                            </li>
                            <li class="@Html.IsActive("LclExps","Index")">
                                <a href="@Url.Action("Index", "LclExps")">
                                    <i class="fa fa-arrow-circle-right" style="margin-left: 5px"></i>
                                    <span style="color: #0173b7">
                                        Lcl Exps
                                    </span>
                                </a>
                            </li>
                            <li class="@Html.IsActive("FclExps","Index")">
                                <a href="@Url.Action("Index", "FclExps")">
                                    <i class="fa fa-arrow-circle-right" style="margin-left: 5px"></i>
                                    <span style="color: #0173b7">
                                        FclExp
                                    </span>
                                </a>
                            </li>
                        </ul>
                    </li>

                    <li class="treeview
                        @Html.IsActive("Producing", "InternalProductionManage,ExternalProductionManage")
                        @Html.IsHidden("ManageEmployee,ManageSupplier,ManageCustomer,ManageProduct")">
                        <a href="#">
                            <i class="fa fa-industry"></i>
                            <span style="color: #0173b7">Import</span>
                            <span class="pull-right-container" style="color: #0173b7">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">

                            <li class="@Html.IsActive("AirImps","Index")">
                                <a href="@Url.Action("Index", "AirImps")">
                                    <i class="fa fa-arrow-circle-right" style="margin-left: 5px"></i>
                                    <span style="color: #0173b7">Air Imports</span>
                                </a>
                            </li>
                            <li class="@Html.IsActive("LclImps","Index")">
                                <a href="@Url.Action("Index", "LclImps")">
                                    <i class="fa fa-arrow-circle-right" style="margin-left: 5px"></i>
                                    <span style="color: #0173b7">
                                        Lcl Import
                                    </span>
                                </a>
                            </li>
                            <li class="@Html.IsActive("FclImps","Index")">
                                <a href="@Url.Action("Index", "FclImps")">
                                    <i class="fa fa-arrow-circle-right" style="margin-left: 5px"></i>
                                    <span style="color: #0173b7">
                                        Fcl Import
                                    </span>
                                </a>
                            </li>
                        </ul>
                    </li>




