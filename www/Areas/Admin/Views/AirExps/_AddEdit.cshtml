                                    <input id="@Html.FieldIdFor(model => model.PaymentId)"
                                           name="@Html.NameFor(model => model.PaymentId)"
                                           value="@Html.ValueFor(model => model.PaymentId)" />
                                    <script>
                                        $(document).ready(function() {
                                            $("#@Html.FieldIdFor(model => model.PaymentId)").kendoComboBox({
                                                filter: "contains",
                                                dataTextField: "Name",
                                                dataValueField: "Value",
                                                placeholder: "Chọn payment...",
                                                minLength: 1,
                                                dataSource: {
                                                    type: "json",
                                                    serverFiltering: true,
                                                    transport: {
                                                        read: {
                                                            url: "/api/LookupApi/GetPayments",
                                                            dataType: "json",
                                                            data: filterPayment
                                                        }
                                                    }
                                                }
                                            });

                                            function filterPayment() {
                                                var data = {
                                                    @*Name: $("#@Html.FieldIdFor(model => model.ShipperId)")
                                                        .data("kendoComboBox").input.val()*@
                                                    Name : ""
                                                };
                                                addAntiForgeryToken(data);
                                                return data;
                                            }
                                        });
                                    </script>
                                    @Html.ValidationMessageFor(model => model.PaymentId)
                                </div>
                                <div class="col-md-1">
                                    <a onclick="openLookupPopup('@Html.FieldIdFor(m => m.PaymentId)', '@LookupTypes.PaymentType')">Add</a>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group form-custom">
                                <div class="col-md-4">
                                    <label class="control-label">Finish:</label>
                                </div>
                                <div class="col-md-7">
                                    @Html.CheckBoxFor(m => m.IsFinish)
                                </div>
                                <div class="col-md-1"></div>

                            </div>
                        </div>

                        <div class="col-md-4">
                            <div class="form-group form-custom">
                                <div class="col-md-4">
                                    <label class="control-label">Notes:</label>
                                </div>
                                <div class="col-md-7">
                                    @Html.FalconTextAreaFor(m => m.Notes)
                                </div>
                                <div class="col-md-1"></div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <input type="submit" id="btnRefreshContactItems" style="display: none" />
        <input type="submit" id="btnRefreshCarrierItems" style="display: none" />
        <input type="submit" id="btnRefreshPortItems" style="display: none" />
        <input type="submit" id="btnRefreshLookupItems" style="display: none" />
    </div>
</div>

<script>
        function openContactPopup(viewId) {

            var url = '/Admin/Contact/ContactAddPopup?viewId=' +
                viewId +
                '&btnId=btnRefreshContactItems&formId=AirExp-edit-form';
            OpenWindow(url, 800, 800, true);
                    }

        function openCarrierPopup(viewId) {
            var url = '/Admin/Carrier/CarrierAddPopup?viewId=' +
                viewId +
                '&btnId=btnRefreshCarrierItems&formId=AirExp-edit-form';
            OpenWindow(url, 800, 800, true);
        }

        function openPortPopup(viewId) {

            var url = '/Admin/Port/PortAddPopup?viewId=' +
                viewId +
                '&btnId=btnRefreshPortItems&formId=AirExp-edit-form';
            OpenWindow(url, 800, 800, true);
        }

        function openLookupPopup(viewId, type) {

            var url = '/Admin/Lookup/LookupAddPopup?viewId=' +
                viewId + "&type=" + type +
                '&btnId=btnRefreshLookupItems&formId=AirExp-edit-form';
            OpenWindow(url, 800, 800, true);
        }

        $(document).ready(function() {

            $('#btnRefreshContactItems')[0].addEventListener("contact",
                function(event) {
                    //refresh combobox
                    $("#@Html.FieldIdFor(m => m.OpicId)").data("kendoComboBox").dataSource.read();
                    $("#" + event.detail.viewId).data("kendoComboBox").value(event.detail.valueId);

                    return false;
                });

            $('#btnRefreshCarrierItems')[0].addEventListener("carrier",
                function(event) {
                    //refresh combobox
                    $("#@Html.FieldIdFor(m => m.AgentId)").data("kendoComboBox").dataSource.read();
                    $("#@Html.FieldIdFor(m => m.CarrierId)").data("kendoComboBox").dataSource.read();
                    $("#" + event.detail.viewId).data("kendoComboBox").value(event.detail.valueId);

                    return false;
                });

            $('#btnRefreshLookupItems')[0].addEventListener("lookup",
                function(event) {
                    //refresh combobox
                    $("#" + event.detail.viewId).data("kendoComboBox").dataSource.read();
                    $("#" + event.detail.viewId).data("kendoComboBox").value(event.detail.valueId);

                    return false;
                });

            $('#btnRefreshPortItems')[0].addEventListener("port",
                function(event) {
                    //refresh combobox
                    console.log(event);
                    $("#@Html.FieldIdFor(m => m.OriginPortId)").data("kendoComboBox").dataSource.read();
                    $("#@Html.FieldIdFor(m => m.DestinationId)").data("kendoComboBox").dataSource.read();
                    $("#@Html.FieldIdFor(m => m.TransitPortId)").data("kendoComboBox").dataSource.read();

                    $("#" + event.detail.viewId).data("kendoComboBox").value(event.detail.valueId);

                    return false;
                });
        });
</script>


